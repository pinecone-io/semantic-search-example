{
  "name": "ts-template",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all format lint test clean && tsc",
    "start": "node src",
    "local": "ts-node src",
    "dev": "nodemon src/index.ts -e ts,json --exec 'npm run local'",
    "lint": "eslint src",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "node",
    "eslint",
    "prettier",
    "jest",
    "supertest"
  ],
  "author": "Roie Schwaber-Cohen",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "@types/papaparse": "^5.3.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^0.0.14",
    "@xenova/transformers": "github:xenova/transformers.js",
    "csv-reader": "^1.0.11",
    "dotenv": "^16.0.3",
    "onnxruntime-node": "^1.14.0",
    "papaparse": "^5.4.1"
  }
}
