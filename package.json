{
  "name": "pinecone-semantic-search",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all format lint && tsc",
    "start": "node build/src",
    "index": "node -r esm build/src/index.js",
    "query": "node --input-type=module build/src/query.js",
    "local": "ts-node src",
    "localQuery": "ts-node src/query.ts",
    "devIndex": "nodemon src/index.ts -e ts,json --exec 'npm run local'",
    "devQuery": "nodemon src/query.ts -e ts,json --exec 'npm run localQuery'",
    "lint": "eslint src",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src"
  },
  "author": "Roie Schwaber-Cohen",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^20.2.3",
    "@types/papaparse": "^5.3.7",
    "@types/uuid": "^9.0.1",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "esm": "^3.2.25",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^0.1.6",
    "@xenova/transformers": "github:xenova/transformers.js",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.0.3",
    "onnxruntime-node": "^1.14.0",
    "papaparse": "^5.4.1",
    "uuid": "^9.0.0",
    "yargs": "^17.7.2"
  }
}
